{
	"name": "buygo/role-permission",
	"description": "Central Core Plugin for BuyGo Ecosystem",
	"type": "wordpress-plugin",
	"version": "0.0.1-beta",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "BuyGo Team",
			"email": "dev@buygo.me"
		}
	],
	"require": {
		"php": ">=7.4",
		"illuminate/support": "^8.0|^9.0|^10.0"
	},
	"autoload": {
		"psr-4": {
			"BuyGo\\Core\\": [
				"app/",
				"includes/"
			]
		},
		"files": [
			"includes/helpers.php",
			"includes/class-buygo-core.php"
		]
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require-dev": {
		"phpunit/phpunit": "^9.0"
	}
}